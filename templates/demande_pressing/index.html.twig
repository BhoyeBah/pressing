{% extends 'base.html.twig' %}

{% block title %}DemandePressing index{% endblock %}

{% block body %}
    <h1>DemandePressing index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Adresse</th>
                <th>Email</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for demande_pressing in demande_pressings %}
            <tr>
                <td>{{ demande_pressing.id }}</td>
                <td>{{ demande_pressing.nom }}</td>
                <td>{{ demande_pressing.adresse }}</td>
                <td>{{ demande_pressing.email }}</td>
                <td>
                    <a href="{{ path('app_demande_pressing_show', {'id': demande_pressing.id}) }}">show</a>
                    <a href="{{ path('app_demande_pressing_edit', {'id': demande_pressing.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_demande_pressing_new') }}">Create new</a>
{% endblock %}
